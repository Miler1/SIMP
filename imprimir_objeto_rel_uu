// String $descricao_alternativa: valor da descricao alternativa a ser utilizada
//
    switch ($nome_atributo) {
    case '{{objeto}}':
        $valor = '';
        {{codigo}}
        if ($return) {
            return $valor;
        }
        echo $valor;
        return;
    }
    return parent::imprimir_objeto_rel_uu($nome_atributo, $return, $imprimir_descricao, $descricao_alternativa);
}